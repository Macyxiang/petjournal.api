generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guardian {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  email     String @unique
  password  String
  phone     String @unique

  @@map("guardian")
}

model LogError {
  id    Int      @id @default(autoincrement())
  stack String
  date  DateTime

  @@map("log_error")
}
