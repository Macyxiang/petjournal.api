generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guardian {
  id                      String  @id @default(uuid())
  firstName               String
  lastName                String
  email                   String  @unique
  password                String
  phone                   String  @unique
  accessToken             String?
  isPrivacyPolicyAccepted Boolean

  @@map("guardian")
}
