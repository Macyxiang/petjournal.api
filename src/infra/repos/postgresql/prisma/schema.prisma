generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guardian {
  id          String  @id @default(uuid())
  firstName   String
  lastName    String
  email       String  @unique
  password    String
  phone       String  @unique
  accessToken String?

  @@map("guardian")
}

model LogError {
  id    Int      @id @default(autoincrement())
  stack String
  date  DateTime

  @@map("log_error")
}
